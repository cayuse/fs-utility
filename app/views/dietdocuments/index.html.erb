<h1>Listing dietdocuments</h1>

<table>
  <thead>
    <tr>
      <th>Diet</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dietdocuments.each do |dietdocument| %>
      <tr>
        <td><%= dietdocument.diet %></td>
        <td><%= link_to 'Show', dietdocument %></td>
        <td><%= link_to 'Edit', edit_dietdocument_path(dietdocument) %></td>
        <td><%= link_to 'Destroy', dietdocument, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dietdocument', new_dietdocument_path %>
